<Window x:Class="WheatherCitis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WheatherCitis"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="1200">
    <Grid>
        <TextBox Name="cityBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="160,13,0,0" VerticalAlignment="Top" Width="75" Click="GetWheather"/>
        <StackPanel Name="stackPan" HorizontalAlignment="Left" Height="425" Margin="10,84,0,0" VerticalAlignment="Top" Width="1172" Orientation="Horizontal"/>

    </Grid>
</Window>
